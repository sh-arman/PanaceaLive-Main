{
  "name": "panacea/panacea",
  "description": "Panacea Laravel application (Laravel 5.1)",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": ">=7.0",
    "laravel/framework": "5.5.*",
    "cartalyst/sentinel": "~2.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpspec/phpspec": "~2.1"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Panacea\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "audit": {
      "block-insecure": false
    },
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  }
}
